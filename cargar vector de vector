
#include "stdafx.h"
#include <iostream>
#include <string>
#include "conio.h"
#include "math.h"
#define MAX 100

using namespace std;

struct vecV 
{
	int subvec[MAX];
};
void cargar (vecV a[],int n1,int n2);

void main()
{
	int n1,n2;
	vecV a[MAX];
	cout<<"INGRESE EL TAMANHO DEL VECTOR EXTERNO"<<endl;
	cin>>n1;
	cout<<"INGRESE EL TAMANHO DEL VECTOR INTERNO"<<endl;
	cin>>n2;
	cargar(a,n1,n2);

	getch();

}

void cargar (vecV a[],int n1,int n2)
{
	int i,j;
	for(i=0;i<n1;i++)
	{
		for(j=0;j<n2;j++)
		{
			cout<<"vector["<<i<<"].subvector["<<j<<"]"<<endl;
			cin>>a[i].subvec[j];
		}
	}
}
